{
  "name": "web3name-promo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "parcel --open",
    "build": "parcel build --reporter @parcel/reporter-bundle-buddy",
    "check": "tsc --noEmit",
    "lint": "yarn run lint-css && yarn run lint-js",
    "lint-js": "eslint --max-warnings 0 --config ./.eslintrc.json \"src/**/*.ts*\"",
    "lint-css": "stylelint --fix \"src/**/*.css\"",
    "css": "tcm --pattern 'src/**/*.module.css' && eslint --fix --config ./.eslintrc.json \"src/**/*.css.d.ts\"",
    "prettify": "prettier --write \"src/**/*.ts*\""
  },
  "dependencies": {
    "@polkadot/api": "^9.5.2",
    "@polkadot/extension-dapp": "^0.44.6",
    "@polkadot/types": "^9.5.2",
    "@polkadot/util": "^10.1.11",
    "@polkadot/util-crypto": "^10.1.11",
    "ky": "^0.32.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "buffer": "^6.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.7.0",
    "prettier": "2.7.1",
    "process": "^0.11.10",
    "stylelint": "^14.14.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^28.0.0",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.8.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "targets": {
    "default": {
      "source": "src/index.html",
      "distDir": "build",
      "context": "browser"
    }
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
